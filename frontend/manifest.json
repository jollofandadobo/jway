{
    "manifest_version": 3,
    "name": "JWAY | AI-Powered Amazon Product Insights",
    "version": "1.0",
    "description": "Extracts the ASIN from Amazon product URLs and sends it to an API.",
    "icons": {
        "16": "icons/logo16.png",
        "48": "icons/logo48.png",
        "128": "icons/logo128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/logo16.png",
            "48": "icons/logo48.png",
            "128": "icons/logo128.png"
        }
    },
    "permissions": [
    "activeTab",
    "tabs"
    ]
}
  